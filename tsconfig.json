{
  "display": "Node 16",
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "outDir": "build",
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
